+package com.android.server.sntpblocker;
+
+import java.util.List;
+
+/** Minimal AIDL interface for SNTP BLOCKER service (v1) */
+interface ISntpBlockerService {
+    /** Set the list of blocked package names (replaces existing list). */
+    void setBlockedApps(in List<String> packages);
+
+    /** Return current blocked package list. */
+    List<String> getBlockedApps();
+
+    /** Enable or disable DNS UI lock (v1: UI disable only). */
+    void enableDnsLock(boolean enable);
+
+    /** Return whether DNS lock is enabled. */
+    boolean isDnsLockEnabled();
+
+    /** Return whether a package is in blocked list. */
+    boolean isPackageBlocked(String pkg);
+}
